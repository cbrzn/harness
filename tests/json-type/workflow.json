{
  "name": "json-type",
  "jobs": {
    "parse": {
      "steps": [
        {
          "uri": "wrap://fs/./build",
          "method": "parse",
          "args": {
            "value": "{\"arg\": { \"foo\": \"foo\", \"bar\": \"bar\" } }"
          }
        },
        {
          "uri": "wrap://fs/./build",
          "method": "stringify",
          "args": {
            "values": [
              "{\"foo\": \"foo\"}",
              "{\"bar\": \"bar\"}"
            ]
          }
        },
        {
          "uri": "wrap://fs/./build",
          "method": "stringifyObject",
          "args": {
            "object": {
              "jsonA": "{\"foo\": \"foo\"}",
              "jsonB": "{\"bar\": \"bar\"}"
            }
          }
        },
        {
          "uri": "wrap://fs/./build",
          "method": "methodJSON",
          "args": {
            "valueA": 5,
            "valueB": "foo",
            "valueC": true
          }
        }
      ]
    }
  }
}